{
  "name": "github-access-control-backend",
  "version": "1.0.0",
  "description": "GitHub Access Control Backend",
  "private": true,
  "main": "src/index.js",
  "scripts": {
    "start": "node src/index.js",
    "dev": "HTTP_PROXY=http://127.0.0.1:1087 HTTPS_PROXY=http://127.0.0.1:1087 nodemon src/index.js",
    "test": "jest",
    "lint": "eslint \"**/*.{js,ts,tsx}\" --cache",
    "lint:fix": "run-s \"lint --fix\"",
    "db:migrate": "node src/scripts/migrate.js",
    "db:seed": "node src/scripts/seed.js",
    "db:reset": "node src/scripts/reset.js",
    "db:check": "node src/scripts/check.js",
    "db:setup": "npm run db:reset && npm run db:seed"
  },
  "type": "module",
  "dependencies": {
    "@octokit/app": "^15.1.6",
    "@octokit/auth-app": "^7.2.1",
    "@octokit/rest": "^21.1.1",
    "@octokit/webhooks": "^13.9.0",
    "axios": "^1.9.0",
    "cors": "^2.8.5",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "helmet": "^6.0.1",
    "json2csv": "^6.0.0-alpha.2",
    "jsonwebtoken": "^9.0.2",
    "morgan": "^1.10.0",
    "mysql2": "^3.2.0",
    "redis": "^4.6.5",
    "sequelize": "^6.31.0",
    "winston": "^3.17.0"
  },
  "devDependencies": {
    "eslint": "^9.26.0",
    "eslint-config-prettier": "^10.1.5",
    "eslint-import-resolver-typescript": "^4.3.4",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-import-alias": "^1.2.0",
    "eslint-plugin-node": "^11.1.0",
    "jest": "^29.5.0",
    "nodemon": "^2.0.22",
    "supertest": "^6.3.3"
  }
}
